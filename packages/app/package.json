    "@types/node": "^20.11.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/three": "^0.161.2",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/coverage-v8": "^1.6.1",
