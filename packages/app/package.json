{
  "name": "@solidtype/app",
  "version": "0.1.0",
  "type": "module",
  "description": "SolidType CAD application with React and three.js",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ai-evals": "RUN_AI_EVALS=true vitest run ai-evals",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:no-userid": "bash scripts/check-no-userid-inputs.sh",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@durable-streams/client": "https://pkg.pr.new/durable-streams/durable-streams/@durable-streams/client@187",
