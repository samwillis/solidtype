    "@durable-streams/state": "https://pkg.pr.new/durable-streams/durable-streams/@durable-streams/state@187",
    "@durable-streams/y-durable-streams": "https://pkg.pr.new/durable-streams/durable-streams/@durable-streams/y-durable-streams@187",
    "@electric-sql/client": "^1.3.1",
    "@fontsource/inter": "^5.2.8",
    "@solidtype/core": "workspace:*",
    "@tanstack/ai": "^0.2.0",
    "@tanstack/ai-client": "^0.2.0",
    "@tanstack/ai-openai": "^0.2.0",
    "@tanstack/electric-db-collection": "^0.2.20",
    "@tanstack/react-db": "^0.1.60",
    "@tanstack/react-form": "^1.27.5",
    "@tanstack/react-router": "^1.120.3",
    "@tanstack/react-start": "^1.145.5",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.4.9",
    "drizzle-orm": "^0.45.1",
    "lib0": "^0.2.108",
    "opencascade.js": "^1.1.1",
    "pg": "^8.16.3",
    "postprocessing": "^6.38.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "remark-breaks": "^4.0.0",
    "three": "^0.161.0",
