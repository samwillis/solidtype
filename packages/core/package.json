{
  "name": "@solidtype/core",
  "version": "0.1.0",
  "type": "module",
  "description": "Functional, data-oriented CAD kernel core",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --format esm --outDir dist --external opencascade.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "bench": "tsx bench/index.ts",
    "bench:model": "tsx bench/model.bench.ts",
    "bench:tessellation": "tsx bench/tessellation.bench.ts",
    "bench:solver": "tsx bench/solver.bench.ts"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^1.6.1",
    "playwright": "^1.57.0",
    "tsdown": "^0.16.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "opencascade.js": "^1.1.1",
    "robust-predicates": "^3.0.2"
  }
}
